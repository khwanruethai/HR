                    <div class="district">
                        @Html.DropDownList("add_district", new List<SelectListItem>
                                                                                    {new SelectListItem { Selected = true, Text = "เลือกแขวง/ตำบล", Value = "1"}
                                                                                    }, new { @class = "form-control select2", @style = "width:100%" })
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="row">
                <div class="col-md-2">
                    รหัสไปรษณีย์
                </div>
                <div class="col-md-10">
                    @Html.TextBox("add_code", "", new { @class = "form-control" })
                </div>
            </div>
        </div>


