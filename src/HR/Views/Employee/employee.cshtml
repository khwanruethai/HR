            $('.data_address').hide();

        }else{
            $('.edit_address').show();
            $('.address_null').hide();
            $('.form_address').hide();
            $('.form_address_update').hide();
            $('.data_address').show();

        }
        $('.form_study').hide();
        $('.form_job').hide();
        $('.form_talent').hide();
        $('.form_train').hide();
        $('.form_family').hide();
        $('.form_kid').hide();

        if($('#check_pay').val() == "0"){

            $('.form_money').hide();
            $('.data_money').hide();

        }else{

            $('.form_money').hide();
            $('.data_money').show();
        }
        $('.form_calendar').hide();

        if($('#check_minus').val() == "0"){

            $('.form_minus').hide();
            $('.data_minus').hide();

        }else{
            $('.form_minus').hide();
            $('.data_minus').show();

        }

        if($('#check_fund').val() == "0"){

            $('.form_fund').hide();
            $('.data_fund').hide();

        }else{

            $('.form_fund').hide();
            $('.data_fund').show();
        }

        if ($('#check_admonish').val() == "0") {

            $('.form_admonish').hide();
            $('.data_admonish').hide();

        }else{

            $('.form_admonish').hide();
            $('.data_admonish').show();
        }

        if ($('#check_commend').val() == "0") {

            $('.form_commend').hide();
            $('.data_commend').hide();

        }else{

            $('.form_commend').hide();
            $('.data_commend').show();
        }

        if ($('#check_contact').val() == "0") {

            $('.form_contact').hide();
            $('.data_contact').hide();
            $('.contact_edit').hide();
            $('.form_contact_edit').hide();
        } else {

            $('.contact_edit').show();
            $('.data_contact').show();
            $('.form_contact').hide();
            $('.form_contact_edit').hide();
            $('.contact_null').hide();
        }

    })
    //emp
    $(document).on('click', '.emp_edit', function(){

        $('.data_emp').hide();
        $('.form_emp_edit').show();

    })
    $(document).on('click','.cancel_emp_edit', function(){

        $('.data_emp').show();
        $('.form_emp_edit').hide();

        var emp = @ViewData["emp_id"];
        $.get('../../Employee/dataForEdit?emp='+emp, function(data){

            var res = data.split('^');
            $('#prefix_th').val(res[1]);
            $('#name_th').val(res[2]);
            $('#last_th').val(res[3]);
            $('#prefix_en').val(res[4]);
            $('#name_en').val(res[5]);
            $('#last_en').val(res[6]);
            $('#type').text(res[7]);
            $('#status').text(res[8]);
            $('#start').val(res[9]);
            $('#end').val(res[10]);

        })


    })
    ///
    $(document).on('click', '.profile_null', function () {

        $('.profile_null').hide();
        $('.form_profile').show();

    })
    $(document).on('click', '.cancel_profile', function () {

        $('.profile_null').show();
        $('.form_profile').hide();
    })
    $(document).on('click', '.cancel_profile_update', function () {

        $('.profile_null').hide();
        $('.form_profile').hide();
        $('.form_profile_update').hide();
        $('.data_profile').show();
    })
    $(document).on('click','.edit_profile', function(){

        $('.profile_null').hide();
        $('.form_profile').hide();
        $('.form_profile_update').show();
        $('.data_profile').hide();
        var emp_id = @ViewData["emp_id"];
        $.get('../../Profile/selectProfile?emp='+emp_id, function(data){
            var res = data.split('^');
            var gender = res[0];
            if(gender == "F"){
                $('.F').prop('checked','checked');
            }else{
                $('.M').prop('checked','checked');
            }
            $('#age_update').val(res[1]);
            $('#nationality_update').val(res[2]);
            $('#race_update').val(res[3]);
            $('#religion_update').val(res[4]);
            $('#blood_update').val(res[5]);
            $('#birthday_update').val(res[6]);
            $('#identification_update').val(res[7]);
            $('#date_issue_update').val(res[8]);
            $('#expired_date_update').val(res[9]);
            $('#weigth_update').val(res[10]);
            $('#height_update').val(res[11]);
            var marital = res[12];
            $('#marital_update').val(marital).selected;
            $('#child_update').val(res[13]);


            //  alert(data);
        })

    })
