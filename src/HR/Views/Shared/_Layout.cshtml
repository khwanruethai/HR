        <script src="../lib/datepicker-th/js/bootstrap-datepicker-thai.js"></script>
        <script src="../lib/datepicker-th/js/locales/bootstrap-datepicker.th.js"></script>
        <script>
            $(document).ready(function () {
                $('.prev').find('i').attr('class', 'fa fa-caret-left');
                $('.next').find('i').attr('class', 'fa fa-caret-right');

                var date = new Date();
              //  alert(date.getDate());
                date.setFullYear(date.getFullYear(), date.getMonth(), 0);
                //date.setFullYear(date.getFullYear(), 11, 31);

               
                $('.datepicker').datepicker({
                    autoclose: true,
                    language: 'th-th',
                    format: 'dd/mm/yyyy',
                    startDate: date
                   // yearRange: "-1000:+0"
                });

                $('.datepicker_normal').datepicker({
                    autoclose: true,
                    language: 'th-th',
                    format: 'dd/mm/yyyy'
                   // yearRange: "-1000:+0"
                });

                $(".datepicker").datepicker("setDate", new Date());

                var menu = $('#menu_now').val();
                if (menu != "") {

                    $('#'+menu).attr('class', 'active');

                } else {

                    $('#menu2').attr('class', 'active');

                }

                ///
                var li = $('#li_now').val();
                if (li != "") {

                    if (li == "li1") {


                        $('#' + li).attr('class', 'active');
                        $('#' + li).find('ul').attr('class', 'nav nav-second-level collapse in');
                        //
                        $('#' + li+'_1').attr('class', 'active');
                        $('#' + li+'_1').find('ul').attr('class', 'nav nav-third-level collapse in');

                    } else {

                        $('#' + li).attr('class', 'active');
                        $('#' + li).find('ul').attr('class', 'nav nav-second-level collapse in');

                    }

                }


                //
            
      

            })
        </script>
        <!--Select2-->
